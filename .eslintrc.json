{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true,
        "node":true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest",
        "allowImportExportEverywhere": true
    },
    "rules": {
        "no-console": "warn",
        "no-use-before-define": "warn"
    }
}
